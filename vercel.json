{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist-trojan",
        "buildCommand": "npm install && npm run build"
      }
    }
  ],
  "routes": [
    { "src": "/demo", "dest": "/demo101.html" },
    { "src": "/demo.html", "dest": "/demo101.html" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
